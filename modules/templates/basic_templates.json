{
  "version": "2.0.0",
  "description": "Universal DOE Platform - Experiment Design Templates",
  "last_updated": "2024-01-01",
  
  "experimental_design_methods": {
    "factorial": {
      "name": {
        "ko": "완전요인설계",
        "en": "Full Factorial Design"
      },
      "description": {
        "ko": "모든 요인의 모든 수준 조합을 실험",
        "en": "All combinations of all factor levels"
      },
      "when_to_use": {
        "ko": [
          "요인 수가 적을 때 (2-4개)",
          "모든 상호작용을 조사하고 싶을 때",
          "예산과 시간이 충분할 때"
        ],
        "en": [
          "Few factors (2-4)",
          "Want to investigate all interactions",
          "Sufficient budget and time"
        ]
      },
      "typical_runs": "2^k (k=factors)",
      "advantages": {
        "ko": ["모든 주효과와 상호작용 추정 가능", "해석이 명확"],
        "en": ["Can estimate all main effects and interactions", "Clear interpretation"]
      },
      "disadvantages": {
        "ko": ["실험 수가 기하급수적으로 증가", "비용이 많이 듦"],
        "en": ["Exponential increase in runs", "Expensive"]
      }
    },
    
    "fractional_factorial": {
      "name": {
        "ko": "부분요인설계",
        "en": "Fractional Factorial Design"
      },
      "description": {
        "ko": "전체 실험의 일부만 수행하는 효율적 설계",
        "en": "Efficient design using fraction of full factorial"
      },
      "when_to_use": {
        "ko": [
          "요인이 많을 때 (5개 이상)",
          "고차 상호작용이 무시 가능할 때",
          "스크리닝 목적"
        ],
        "en": [
          "Many factors (5+)",
          "High-order interactions negligible",
          "Screening purpose"
        ]
      },
      "resolution": {
        "III": {
          "ko": "주효과가 2차 상호작용과 교락",
          "en": "Main effects confounded with 2-way interactions"
        },
        "IV": {
          "ko": "주효과는 명확, 2차 상호작용끼리 교락",
          "en": "Main effects clear, 2-way interactions confounded"
        },
        "V": {
          "ko": "주효과와 2차 상호작용 모두 명확",
          "en": "Main effects and 2-way interactions clear"
        }
      }
    },
    
    "plackett_burman": {
      "name": {
        "ko": "Plackett-Burman 설계",
        "en": "Plackett-Burman Design"
      },
      "description": {
        "ko": "많은 요인의 주효과를 효율적으로 스크리닝",
        "en": "Efficient screening of main effects for many factors"
      },
      "when_to_use": {
        "ko": [
          "요인이 매우 많을 때 (8-50개)",
          "중요한 요인을 찾는 초기 단계",
          "상호작용이 무시 가능할 때"
        ],
        "en": [
          "Very many factors (8-50)",
          "Initial stage to find important factors",
          "Interactions negligible"
        ]
      },
      "typical_runs": "Multiple of 4",
      "limitations": {
        "ko": "상호작용 추정 불가",
        "en": "Cannot estimate interactions"
      }
    },
    
    "central_composite": {
      "name": {
        "ko": "중심합성설계",
        "en": "Central Composite Design"
      },
      "description": {
        "ko": "2차 반응표면 모델을 위한 설계",
        "en": "Design for quadratic response surface models"
      },
      "when_to_use": {
        "ko": [
          "최적화 단계",
          "곡률이 있는 반응표면",
          "2-5개 요인"
        ],
        "en": [
          "Optimization phase",
          "Curved response surface",
          "2-5 factors"
        ]
      },
      "components": {
        "factorial": {
          "ko": "요인점: 2^k개",
          "en": "Factorial points: 2^k"
        },
        "axial": {
          "ko": "축점: 2k개",
          "en": "Axial points: 2k"
        },
        "center": {
          "ko": "중심점: 반복 실험용",
          "en": "Center points: for replication"
        }
      },
      "alpha_values": {
        "rotatable": "sqrt(k)",
        "orthogonal": "depends on design",
        "face_centered": "1"
      }
    },
    
    "box_behnken": {
      "name": {
        "ko": "Box-Behnken 설계",
        "en": "Box-Behnken Design"
      },
      "description": {
        "ko": "극단 조건을 피하는 2차 반응표면 설계",
        "en": "Response surface design avoiding extreme conditions"
      },
      "when_to_use": {
        "ko": [
          "3개 이상의 요인",
          "극단 조건이 위험하거나 비싸질 때",
          "2차 모델이 필요할 때"
        ],
        "en": [
          "For 3 or more factors",
          "When extreme conditions are risky or expensive",
          "When quadratic model is needed"
        ]
      },
      "characteristics": {
        "ko": [
          "모든 요인이 3수준",
          "꼭짓점 실험 없음",
          "회전 가능 설계"
        ],
        "en": [
          "All factors at 3 levels",
          "No corner experiments",
          "Rotatable design"
        ]
      }
    },
    
    "mixture_design": {
      "name": {
        "ko": "혼합물 설계",
        "en": "Mixture Design"
      },
      "description": {
        "ko": "성분의 합이 100%인 혼합물 실험",
        "en": "Experiments where components sum to 100%"
      },
      "types": {
        "simplex_lattice": {
          "ko": "심플렉스 격자 설계",
          "en": "Simplex Lattice Design"
        },
        "simplex_centroid": {
          "ko": "심플렉스 중심 설계",
          "en": "Simplex Centroid Design"
        },
        "extreme_vertices": {
          "ko": "극단 꼭짓점 설계",
          "en": "Extreme Vertices Design"
        }
      },
      "when_to_use": {
        "ko": [
          "용매 혼합 비율 최적화",
          "합금 조성 설계",
          "제형 개발"
        ],
        "en": [
          "Solvent mixture optimization",
          "Alloy composition design",
          "Formulation development"
        ]
      }
    },
    
    "d_optimal": {
      "name": {
        "ko": "D-최적 설계",
        "en": "D-Optimal Design"
      },
      "description": {
        "ko": "정보 행렬식을 최대화하는 설계",
        "en": "Maximizes determinant of information matrix"
      },
      "when_to_use": {
        "ko": [
          "제약 조건이 있을 때",
          "비표준 설계 영역",
          "실험 수를 최소화해야 할 때"
        ],
        "en": [
          "With constraints",
          "Non-standard design regions",
          "Need to minimize number of runs"
        ]
      },
      "advantages": {
        "ko": [
          "유연한 실험 수",
          "복잡한 제약 처리 가능",
          "모델 특화 최적화"
        ],
        "en": [
          "Flexible run size",
          "Handles complex constraints",
          "Model-specific optimization"
        ]
      }
    }
  },
  
